{
	"name": "www.chen.software",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"prepare": "panda codegen",
		"dev": "tinacms dev -c \"next dev\"",
		"build": "tinacms build && next build",
		"build-local": "tinacms build --local --skip-indexing --skip-cloud-checks && next build",
		"start": "tinacms build && next start",
		"format": "npx @biomejs/biome format --write ./app",
		"lint:biome": "npx @biomejs/biome lint --write ./app",
		"lint:next": "next lint",
		"lint": "npm run lint:biome && npm run lint:next"
	},
	"dependencies": {
		"@ark-ui/anatomy": "^3.5.0",
		"@ark-ui/react": "^5.4.0",
		"next": "^14.2.25",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tinacms": "^2.7.3",
		"typescript": "5.8.2"
	},
	"devDependencies": {
		"@pandacss/dev": "^0.53.3",
		"@park-ui/panda-preset": "^0.43.1",
		"@tinacms/cli": "^1.9.3",
		"@types/node": "^22.13.13",
		"eslint": "^9.23.0",
		"eslint-config-next": "^15.2.3"
	}
}
